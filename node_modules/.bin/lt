#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bb/Pulpit/appl/streaming-diagram-server/node_modules/.pnpm/localtunnel@2.0.2/node_modules/localtunnel/bin/node_modules:/home/bb/Pulpit/appl/streaming-diagram-server/node_modules/.pnpm/localtunnel@2.0.2/node_modules/localtunnel/node_modules:/home/bb/Pulpit/appl/streaming-diagram-server/node_modules/.pnpm/localtunnel@2.0.2/node_modules:/home/bb/Pulpit/appl/streaming-diagram-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/bb/Pulpit/appl/streaming-diagram-server/node_modules/.pnpm/localtunnel@2.0.2/node_modules/localtunnel/bin/node_modules:/home/bb/Pulpit/appl/streaming-diagram-server/node_modules/.pnpm/localtunnel@2.0.2/node_modules/localtunnel/node_modules:/home/bb/Pulpit/appl/streaming-diagram-server/node_modules/.pnpm/localtunnel@2.0.2/node_modules:/home/bb/Pulpit/appl/streaming-diagram-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../localtunnel/bin/lt.js" "$@"
else
  exec node  "$basedir/../localtunnel/bin/lt.js" "$@"
fi
